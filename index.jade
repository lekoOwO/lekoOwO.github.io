//- 技能模板
mixin skill(content) 
    skill
        block
        .content=content
mixin skillUnlocking(content) 
    skill.unlocking
        i.icons
            block
            i.lock.icon
        .content=content
//- 作品模板
mixin works(img, header, description, link, comment) 
    a.ts.card(href=link)
        .image
            img.lozad(data-src=img class="thumbnail")
        .content
            .header=header
            .description=description
            .comment=comment

//- 次要作品模板
mixin subworks(header, description, link)
    a.item(href=link)
        .ts.header=header
        p=description

